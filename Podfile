# Uncomment the next line to define a global platform for your project

platform :ios, '10.0'

def myPods

# Pods for All the Targets

pod 'TOCropViewController'
pod 'SwiftGifOrigin'
pod 'Permission/Camera'
pod 'Permission/Photos'
pod 'Permission/MediaLibrary'
pod 'Permission/Microphone'
pod 'ABVideoRangeSlider-SWIFT-5'

end 


target 'CustomCamera' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CustomCamera
  myPods
  
end




target 'CustomCameraTests' do
  # inherit! :search_paths
  # Pods for testing
  
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CustomCamera
  myPods
  
end

target 'CustomCameraUITests' do
  # Pods for testing
  
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CustomCamera
  myPods
  
end


